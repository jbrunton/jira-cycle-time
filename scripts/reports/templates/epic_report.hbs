<table class="aui">
  <tr>
    <th>Key</th>
    <th>Summary</th>
    <th>Started</th>
    <th>Completed</th>
  </tr>
  
  {{#each issues}}
  <tr>
    <td class="ghx-nowrap ghx-minimal">
      <a href="/browse/{{key}}">
        {{key}}
      </a>
    </td>
    <td>
      {{summary}}
    </td>
    <td>
      {{date startedDate}}
    </td>
    <td>
      {{date completedDate}}
    </td>
  </tr>
  {{/each}}
</table>
