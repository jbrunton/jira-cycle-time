<table class="aui">
  <tr>
    <th width="20%" class="ghx-nowrap">Date</th>
    <th width="30%">Event</th>
    <th width="22%">Summary</th>
    <th width="22%">WIP</th>
  </tr>

  {{#each events}}
  <tr>
    <td width="20%" class="ghx-nowrap">{{date date}}</td>
    <td width="30%">{{key}}</td>
    <td width="22%">{{epic.summary}}</td>
    <td width="22%">{{wip}}</td>
  </tr>
  {{/each}}

</table>
